{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "api/game.ts": {
      "maxDuration": 30
    },
    "api/resolve-predictions.ts": {
      "maxDuration": 30
    },
    "api/system/index.ts": {
      "maxDuration": 30
    }
  },
  "ignoreCommand": "echo 'Ignoring api-shared directory'",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
